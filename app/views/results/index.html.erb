<p id="notice"><%= notice %></p>

<h5>报名信息</h5>
<div class="row">
    <div class="col s12">
	<%  @openid_results.each do |result| %>

	    <% entry = Entry.new(result: result).values %>

	    <div class="card">
		<div class="card-content">
		    <span class="card-title"><%= result.form_name %></span>
		    <dl class="detail" >
			<dt>	姓名：</dt>
			<dd><%= entry['姓名']%></dd>
			<dt>序号: </dt>
			<dd><%= entry['序号'] %></dd>
			<% if entry['广西区内报名'].to_s.match(/\d/) %>
			    <dt>广西区内报名:</dt>
			    <dd><%= compact_empty(entry['广西区内报名']) %></dd>
			<% end %>
			<% if entry['广西区外报名'].to_s.match(/\d/) %>
			    <dt>广西区外报名:</dt>
			    <dd><%= compact_empty(entry['广西区外报名']) %></dd>
			<% end %>
			<% if entry['作品评审费'].to_s.match(/\d/) %>
			    <dt>作品评审费:</dt>
			    <dd><%= compact_empty(entry['作品评审费']) %></dd>
			<% end %>
			<%# if entry.keys.select { |k| %w(广西区内报名 广西区外报名 作品评审费).include?(k) && entry[k].to_s.match(/\d/) } %>
			<%# end %>
			<dt>确认码：</dt>
			<dd><%= entry['确认码'] %></dd>
			<dt>提交时间：</dt>
			<dd><%= entry['提交时间'] %></dd>
			<div class="card-action">
			    <%= link_to '查看详情',{ controller: "results",action: 'show',id: result,  subject: result.class.name}, class: "waves-effect waves-light btn blue darken-1 white-text" %>
			</div>
		    </dl>
		</div>

	    </div>

	<% end %>
	<%= link_to '返回',  root_path, class: "waves-effect waves-light btn blue darken-1 white-text" %>
    </div>
</div>

<%#= link_to 'New Result', new_result_path %>

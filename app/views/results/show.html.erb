<p id="notice"><%= notice %></p>
<div class="row">
    <div class="col s12">
	<div class="card">
	    <div class="card-content">
		<table class="striped">
		    <thead>
			<tr>
			    <th class="card-title"><%=@result.form_name%></th>
			</tr>
		    </thead>

		    <tbody>
			<%#  reject_rows(Entry.new(result: @result).values).each do |k, v| %>
			<%  Entry.new(result: @result).values.each do |k, v| %>
			    <%# unless v.to_s.chomp.empty? %>
			    <tr>
				<td><b><%= "#{k}:"%></b><%= v %></td>
			    </tr>
			    <%# end %>
			<% end %>
		    </tbody>
		</table>
		
	    </div>
	</div>
	<%= link_to '返回', { controller: "results",action: 'index', subject: @result.class.name}, class: "waves-effect waves-light btn blue darken-1 white-text" %>
    </div>
</div>


